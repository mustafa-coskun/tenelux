# Server Configuration
NODE_ENV=development
PORT=3001

# Database
DB_PATH=./data/tenelux.db

# WebSocket
WS_PORT=3001

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001

# ============================================
# AD CONFIGURATION
# ============================================

# Enable/Disable Ads
REACT_APP_ADS_ENABLED=true

# Ad Provider (ezoic - AI-powered optimization)
REACT_APP_AD_PROVIDER=ezoic

# Ezoic Site ID (Get from Ezoic dashboard after integration)
REACT_APP_EZOIC_SITE_ID=your_site_id

DISABLE_ESLINT_PLUGIN="true"
