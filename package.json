{
  "name": "tenelux-game",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.25",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/sqlite3": "^3.1.11",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "bcrypt": "^5.1.1",
    "express": "^5.1.0",
    "i18next": "^25.6.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.2.3",
    "react-scripts": "5.0.1",
    "sqlite3": "^5.1.7",
    "sweetalert2": "^11.26.3",
    "typescript": "^4.9.5",
    "uuid": "^13.0.0",
    "web-vitals": "^2.1.4",
    "ws": "^8.18.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "test:performance": "react-scripts test --testNamePattern=\"Performance\" --watchAll=false",
    "test:security": "react-scripts test --testNamePattern=\"Security\" --watchAll=false",
    "test:comprehensive": "react-scripts test --testPathPattern=\"performance-security-comprehensive\" --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,css,md}",
    "server": "node server.js",
    "server:ts": "ts-node server.js",
    "dev": "concurrently \"npm run server\" \"npm start\"",
    "build:full": "npm run build && npm run server",
    "db:seed:dev": "NODE_ENV=development node -e \"require('./src/database/developmentSeed').seedDevelopmentData()\"",
    "db:cleanup:dev": "NODE_ENV=development node src/database/cleanup.js cleanup-dev",
    "db:cleanup": "node src/database/cleanup.js cleanup",
    "db:optimize": "node src/database/cleanup.js optimize",
    "db:health": "node src/database/cleanup.js health",
    "db:maintenance": "node src/database/cleanup.js maintenance",
    "db:migrate": "node -e \"require('./src/database/DatabaseManager').initializeDatabaseManager().then(m => m.migrate(require('./src/database/migrations').migrations))\"",
    "security:audit": "npm audit --audit-level moderate",
    "performance:test": "npm run test:performance",
    "migration:test": "npm run test -- --testNamePattern=\"Migration|Database Security\"",
    "production:check": "npm run security:audit && npm run test && npm run lint"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(uuid)/)"
    ],
    "moduleNameMapper": {
      "^uuid$": "uuid"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/express-rate-limit": "^6.0.2",
    "compression": "^1.8.1",
    "concurrently": "^8.2.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "express-rate-limit": "^7.5.1",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2"
  }
}
