# Production Environment Variables

# Server Configuration
NODE_ENV=production
PORT=3001

# Database
DB_PATH=./data/tenelux.db

# WebSocket
WS_PORT=3001

# CORS
ALLOWED_ORIGINS=https://tenelux.site

# Session Secret
SESSION_SECRET=CHANGE_THIS_TO_A_RANDOM_SECRET_KEY

# ============================================
# AD CONFIGURATION
# ============================================

# Enable Ads
REACT_APP_ADS_ENABLED=true
REACT_APP_AD_PROVIDER=propellerads

# PropellerAds Zone IDs
REACT_APP_PROPELLER_BANNER_ZONE=your_banner_zone_id
REACT_APP_PROPELLER_INTERSTITIAL_ZONE=your_interstitial_zone_id
REACT_APP_PROPELLER_PUSH_ZONE=your_push_zone_id
REACT_APP_PROPELLER_ONCLICK_ZONE=your_onclick_zone_id

DISABLE_ESLINT_PLUGIN="true"
